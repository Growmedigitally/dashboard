{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@molecules/*": ["src/components/molecules/*"],
      "@atoms/*": ["src/components/atoms/*"],
      "@organisms/*": ["src/components/organisms/*"],
      "@sections/*": ["src/components/organisms/sections/*"],
      "@template/*": ["src/components/templates/*"],
      "@svgs/*": ["src/components/molecules/svgElements/*"],
      "@styles/*": ["styles/*"],
      "@atomsCSS/*": ["styles/components/atoms/*"],
      "@moleculesCSS/*": ["styles/components/molecules/*"],
      "@organismsCSS/*": ["styles/components/organisms/*"],
      "@templatesCSS/*": ["styles/components/templates/*"],
      "@constant/*": ["src/constants/*"],
      "@reduxStore/*": ["src/redux/*"],
      "@type/*": ["src/types/*"],
      "@hook/*": ["src/hooks/*"],
      "@store/*": ["src/redux/stateProvider/*"],
      "@util/*": ["src/utils/*"],
      "@apiService/*": ["pages/apiService/*"],
      "@assets/*": ["public/assets/*"],
      "@objectPropertiesEditor/*": [
        "src/components/templates/craftBuilder/objectPropertiesEditor/*"
      ],
      "@craftBuilder/*": ["src/components/templates/craftBuilder/*"]
    },
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveConstEnums": true,
    "strictFunctionTypes": false,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": false
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.svg",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
